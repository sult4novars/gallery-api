{
  "compilerOptions": {
    "allowSyntheticDefaultImports" : true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "CommonJS",
    "target": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"]
    }
  },
  "exclude":[
    "./node_modules"
  ]
}
